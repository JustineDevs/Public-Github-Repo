{
  "name": "public-github-repo",
  "version": "1.0.0",
  "description": "A curated collection of helpful repositories for developers, featuring resources on AI, Large Language Models (LLMs), and much more.",
  "main": "index.js",
  "scripts": {
    "commit": "node scripts/parallel-commit.js",
    "commit:dry": "node scripts/parallel-commit.js --dry-run",
    "commit:sh": "bash scripts/parallel-commit.sh",
    "commit:sh:dry": "bash scripts/parallel-commit.sh --dry-run",
    "commit:all": "git add . && git commit -m 'chore: update multiple files'",
    "commit:auto": "node scripts/parallel-commit.js --max 3",
    "status": "git status --porcelain",
    "changes": "git diff --name-only",
    "staged": "git diff --cached --name-only"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "llm",
    "development",
    "resources",
    "tools",
    "libraries",
    "tutorials",
    "courses"
  ],
  "author": "JustineDevs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustineDevs/Public-Github-Repo.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {},
  "dependencies": {}
}
